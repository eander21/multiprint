<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="PentaPrint.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="PentaPrint.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
    </startup>
    <applicationSettings>
        <PentaPrint.Properties.Settings>
            <setting name="ApplicationName" serializeAs="String">
                <value>MultiPrint</value>
            </setting>
            <setting name="InputFields" serializeAs="Xml">
                <value>
                    <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <string>MEP1</string>
                    </ArrayOfString>
                </value>
            </setting>
            <setting name="PrintValidation" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="ResolverOffsetPattern" serializeAs="String">
                <value>^P[0-9]*.*?#S.*?[#].*V[a-zA-Z0-9]{5}#Z[a-zA-Z0-9]{4}(?&lt;resolv&gt;[a-zA-Z0-9]{4})[a-zA-Z0-9]{4}#$</value>
            </setting>
        </PentaPrint.Properties.Settings>
    </applicationSettings>
    <userSettings>
        <PentaPrint.Properties.Settings>
            <setting name="PrinterPort" serializeAs="String">
                <value>COM6</value>
            </setting>
            <setting name="PrinterBaud" serializeAs="String">
                <value>9600</value>
            </setting>
            <setting name="LaserMarkingPattern" serializeAs="String">
                <value>^P(?&lt;partnumber&gt;[0-9]*)#T(?&lt;serialnumber&gt;.*)#.*V[a-zA-Z0-9]{5}#$</value>
            </setting>
        </PentaPrint.Properties.Settings>
    </userSettings>
</configuration>
